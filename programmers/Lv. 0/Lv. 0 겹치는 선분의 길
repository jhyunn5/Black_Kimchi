def solution(lines):
    check = {}
    for i in lines:
        for j in range(min(i[0],i[1]), max(i[0], i[1])):
            if j not in check:
                check[j] = 0
            else:
                check[j] += 1
    answer = 0
    for c in check:
        if check[c] >= 1:
            answer += 1
    return  answer
